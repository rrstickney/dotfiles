configuration {
  modi: "drun,run,window";
  show-icons: true;
  icon-theme: "Papirus";
  drun-display-format: "{name}";
  display-drun: "Apps";
  display-run: "Run";
  display-window: "Windows";
  sidebar-mode: true;

  /* If you use Nerd Fonts, this is a nice default */
  font: "SpaceMono Nerd Font 12";
}

/* Load theme */
@theme "midnight-glacial"

